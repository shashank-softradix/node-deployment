{
  "swagger": "2.0",
  "info": {
    "description": "dummy_Project Api's.",
    "version": "1.0.3",
    "title": "dummy_Project"
  },
  "host": "localhost:3000",
  "basePath": "",
  "schemes": ["http", "https"],
  "securityDefinitions": {
