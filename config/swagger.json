          "summary": "APi for get user list/pag",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "required": true,
              "type": "object",
              "schema": {
                "properties": {
                  "length": {
                    "type": "string"
                  },
                  "limit": {
                    "type": "string"
